Algoritmo sin_titulo
	Escribir 'Ingrese un número en segundos';
	Leer SegundosIngresados;
	Minutos <- SegundosIngresados /60;
	Segundos <- SegundosIngresados MOD 60;
	Escribir 'Su Número ingresado en minutos es:', Minutos, 'min';
	Escribir 'Su Número ingresado en segundos es:', Segundos, 'seg';
FinAlgoritmo
