Algoritmo sin_titulo
	Escribir 'Ingrese el primer valor';
	Escribir 'Ingrese el segundo valor';
	Definir Valor1, Valor2 Como Entero;
	Leer Valor1, Valor2;
	Cociente <- Valor1/Valor2;
	Escribir 'El Cociente del Valor 1 es:', Cociente;
	Resto <- Cociente/Valor2;
	Escribir 'El resto del valor 1 es:', Resto;
FinAlgoritmo
